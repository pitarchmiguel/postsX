{
  "crons": [
    {
      "path": "/api/scheduler/run",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/metrics/cron",
      "schedule": "*/15 * * * *"
    }
  ],
  "functions": {
    "app/(dashboard)/dashboard/page.tsx": {
      "maxDuration": 10
    },
    "app/api/*/route.ts": {
      "maxDuration": 10
    }
  }
}
